        <TextBlock HorizontalAlignment="Left" Margin="614,34,0,0" TextWrapping="Wrap" Text="Offset Time" VerticalAlignment="Top" Width="82"/>
        <Button Content="..." HorizontalAlignment="Left" Margin="855,93,0,0" VerticalAlignment="Top" Width="21" Click="Button_Click_3"/>
        <TextBox x:Name="File_Copy1" HorizontalAlignment="Left" Margin="528,93,0,197" TextWrapping="Wrap" Text="" Width="313"/>
